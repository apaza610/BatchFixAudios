<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="title" translatable="yes">BatchFixAudios</property>
    <property name="width">252</property>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">252</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label1">
            <property name="justify">left</property>
            <property name="relief">raised</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Path of Tutorial Folder:</property>
            <property name="width">30</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">x</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="taPathTutorial" named="True">
            <property name="text" translatable="yes">E:\win\Videos</property>
            <property name="width">30</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btnBatchFix" named="True">
            <property name="command" type="command" cbtype="simple">batch_fix_audios</property>
            <property name="text" translatable="yes">batchFix</property>
            <layout manager="pack">
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
